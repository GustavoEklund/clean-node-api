{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "NodeJS API using Clean Architecture",
  "main": "index.js",
  "scripts": {
	"test": "jest --watchAll",
	"test:staged": "jest"
  },
  "lint-staged": {
    "*.js": [
		"standard --fix",
		"npm run test:staged",
        "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "standard": {
	"env": ["jest"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GustavoEklund/clean-node-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GustavoEklund/clean-node-api/issues"
  },
  "homepage": "https://github.com/GustavoEklund/clean-node-api#readme",
  "devDependencies": {
    "husky": "^5.0.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "standard": "^16.0.3"
  }
}
